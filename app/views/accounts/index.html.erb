<h3>This is account's index page</h3>


<h3>Accounts</h3>

<table>
  <thead>
      <tr>
        <th>Account number</th>
        <th>Account type</th>
        <th>Action</th>
      </tr>
  </thead>
  <tbody>
        <% @accounts.each do |account| %>
          <tr>
            <td><%= link_to account.acc_no %></td>
            <td><%= account.acc_type %></td>
            <td><%= link_to 'Edit' %></td>
            <td><%= link_to 'Delete' %></td>
          </tr>
        <% end %>
  </tbody>
</table>
